          "empFName": {
            "type": "string",
            "nullable": true
          },
          "empMName": {
            "type": "string",
            "nullable": true
          },
          "empLName": {
            "type": "string",
            "nullable": true
          },
          "empAdd": {
            "type": "string",
            "nullable": true
          },
          "empAge": {
